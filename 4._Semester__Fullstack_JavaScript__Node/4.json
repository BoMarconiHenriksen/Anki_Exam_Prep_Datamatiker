        }, 
        {
            "__type__": "Note", 
            "data": "", 
            "fields": [
                "Hvad returner&nbsp;fs.readFileSync('/path/to/file')", 
                "Et buffer objekt, der indeholder hele indeholdet af filen."
            ], 
            "flags": 0, 
            "guid": "n`rJwj*xD%", 
            "note_model_uuid": "5d8b7104-664b-11e8-9224-f0038c751458", 
