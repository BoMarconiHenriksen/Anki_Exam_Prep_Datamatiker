        {
            "__type__": "Note", 
            "data": "", 
            "fields": [
                "Hvilket modul skal du bruge for at udfører en filesystem operation?", 
                "Du skal bruge fs modulet fra Node core library."
            ], 
            "flags": 0, 
            "guid": "GfkInyA_(d", 
            "note_model_uuid": "5d8b7104-664b-11e8-9224-f0038c751458", 
