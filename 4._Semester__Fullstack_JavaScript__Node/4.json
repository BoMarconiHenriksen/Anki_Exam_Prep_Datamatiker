        }, 
        {
            "__type__": "Note", 
            "data": "", 
            "fields": [
                "Hvordan loades filesystem modulet(fs)?", 
                "var fs = require('fs');"
            ], 
            "flags": 0, 
            "guid": "Keek+:i3FC", 
            "note_model_uuid": "5d8b7104-664b-11e8-9224-f0038c751458", 
            "tags": [
